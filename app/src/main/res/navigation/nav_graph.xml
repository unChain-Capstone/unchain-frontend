<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/behaviorFragment">

    <fragment
        android:id="@+id/behaviorFragment"
        android:name="com.unchain.ui.behavior.BehaviorFragment"
        android:label="BehaviorFragment">
        <action
            android:id="@+id/action_behaviorFragment_to_inputDataFragment"
            app:destination="@id/inputDataFragment" />
        <action
            android:id="@+id/action_behaviorFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_behaviorFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/inputDataFragment"
        android:name="com.unchain.ui.input.InputDataFragment"
        android:label="InputDataFragment" />

    <fragment
        android:id="@+id/historyFragment"
        android:name="com.unchain.ui.history.HistoryFragment"
        android:label="HistoryFragment" />

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.unchain.ui.home.HomeFragment"
        android:label="HomeFragment" />

</navigation>
